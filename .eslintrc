{
  "env": {
    "node": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "comma-dangle": [2, "never"],
    "comma-spacing": [2, { "before": false, "after": true }],
    "curly": [2, "all"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "keyword-spacing": 2,
    "new-cap": [2, { "capIsNew": false, "newIsCap": true }],
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "all", {
        "nestedBinaryExpressions": false,
        "conditionalAssign": false
    }],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-warning-comments": [2, { "terms": ["he", "his", "he's", "him", "himself", "she", "hers", "she's", "her", "herself"], "location": "anywhere" }],
    "no-with": 2,
    "one-var": [2, "never"],
    "object-curly-spacing": [ 2, "always" ],
    "quotes": [2, "single", "avoid-escape"],
    "radix": 2,
    "semi": 2,
    "space-before-blocks": [2, "always"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "strict": [2, "never"],
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": 2,
    "yoda": [2, "never"]
  }
}
